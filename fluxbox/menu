# fluxbox menu goes in ~/.fluxbox/menu
[begin] (fluxbox)
[exec] (XTrackCAD) {bash -c "exec $(find /usr -path '*/bin/*' -name 'xtrkcad*')"} <xtrkcad>
[exec] (XTrackCAD Doc) {xdg-open $(find /usr -name 'contents.html')} <Web Browser>
[exec] (File Manager) {bash -c "exec $(find /usr -path '*/bin/*' -name thunar -o -name rox-filer)" }
[restart] (Restart Window Mgr) {/usr/bin/startfluxbox}
[separator]
[include] (/etc/X11/fluxbox/fluxbox-menu)
[end]
