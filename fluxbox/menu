# fluxbox menu goes in ~/.fluxbox/menu
[begin] (fluxbox)
[exec] (XTrackCAD) {xtrkcad} <xtrkcad>
[exec] (XTrackCAD Doc) {xdg-open /usr/share/xtrkcad/html/contents.html} <Web Browser>
[exec] (File Manager) {rox-filer}
[restart] (Restart Window Mgr) {/usr/bin/startfluxbox}
[separator]
[include] (/etc/X11/fluxbox/fluxbox-menu)
[end]
